<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Skibidi Redirect</title>
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            document.addEventListener('keydown', function(event) {
                const keyPressed = event.key.toLowerCase();
                const userInput = document.getElementById('skibidiInput');
                
                // Check if Enter is pressed and input is "skibidi"
                if (event.key === "Enter" && userInput.value.toLowerCase() === "skibidi") {
                    window.location.href = "https://static.wikia.nocookie.net/skibidi-toilet-official/images/b/b1/GiantST.png/revision/latest/thumbnail/width/360/height/360?cb=20240205231829";
                }
            });
        });
    </script>
</head>
<body>
    <h1>Type "skibidi" and press Enter</h1>
    <input type="text" id="skibidiInput" placeholder="Type skibidi here">
</body>
</html>
